{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$defs": {
    "s3Uri": {
      "type": "string",
      "pattern": "^s3://[a-zA-Z0-9_-]+/[a-zA-Z0-9_./-]+"
    },
    "s3UriDirectory": {
      "type": "string",
      "pattern": "^s3://[a-zA-Z0-9_-]+/[a-zA-Z0-9_./-]+/$"
    },
    "multiqcParquetFileObject": {
      "type": "object",
      "properties": {
        "fastqId": {
          "type": "string",
          "examples": ["FASTQ12345"]
        },
        "libraryId": {
          "type": "string",
          "examples": ["LIB12345"]
        },
        "lane": {
          "type": "integer",
          "examples": [1]
        },
        "multiqcParquetFileUri": {
          "$ref": "#/$defs/s3Uri"
        }
      },
      "required": ["fastqId", "libraryId", "lane", "multiqcParquetFileUri"]
    },
    "tags": {
      "type": "object",
      "properties": {
        "instrumentRunId": {
          "type": "string",
          "examples": ["251003_A00130_0384_AHL7LWDSXF"]
        },
        "libraryIdList": {
          "type": "array",
          "items": {
            "type": "string",
            "examples": ["LIB12345", "LIB67890"]
          }
        }
      },
      "required": ["instrumentRunId", "libraryIdList"]
    },
    "inputs": {
      "type": "object",
      "properties": {
        "instrumentRunId": {
          "type": "string",
          "examples": ["251003_A00130_0384_AHL7LWDSXF"]
        },
        "interOpDirectory": {
          "type": "string",
          "examples": [1234567890]
        },
        "bclConvertReportDirectory": {
          "type": "string",
          "examples": ["Report directory"]
        },
        "additionalMultiQcDataFiles": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/multiqcParquetFileObject"
          }
        }
      },
      "required": ["instrumentRunId", "interOpDirectory", "bclConvertReportDirectory"]
    },
    "engineParameters": {
      "type": "object",
      "properties": {
        "projectId": {
          "type": "string"
        },
        "pipelineId": {
          "type": "string"
        },
        "outputUri": {
          "$ref": "#/$defs/s3UriDirectory"
        },
        "logsUri": {
          "$ref": "#/$defs/s3UriDirectory"
        },
        "cacheUri": {
          "$ref": "#/$defs/s3UriDirectory"
        }
      },
      "required": ["projectId", "pipelineId", "outputUri", "logsUri", "cacheUri"]
    }
  },
  "type": "object",
  "properties": {
    "tags": {
      "$ref": "#/$defs/tags"
    },
    "inputs": {
      "$ref": "#/$defs/inputs"
    },
    "engineParameters": {
      "$ref": "#/$defs/engineParameters"
    }
  },
  "required": ["tags", "inputs", "engineParameters"]
}
